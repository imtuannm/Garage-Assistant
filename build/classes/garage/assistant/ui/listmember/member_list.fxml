<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="garage.assistant.ui.listmember.MemberListController">
    <stylesheets>
        <URL value="@member_list.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tblView" prefHeight="400.0" prefWidth="600.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="idCol" prefWidth="75.0" text="Member ID" />
            <TableColumn fx:id="nameCol" prefWidth="75.0" text="Full Name" />
            <TableColumn fx:id="mobileCol" prefWidth="75.0" text="Mobile" />
            <TableColumn fx:id="emailCol" prefWidth="75.0" text="Email" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
