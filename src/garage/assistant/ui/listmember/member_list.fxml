<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="garage.assistant.ui.listmember.MemberListController">
   <children>
      <VBox prefHeight="472.0" prefWidth="769.0">
         <children>
            <JFXTextField fx:id="keyword" labelFloat="true" onAction="#handleSearchOperation" promptText="Search">
               <VBox.margin>
                  <Insets bottom="5.0" left="250.0" right="250.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <TableView fx:id="tblView" prefHeight="446.0" prefWidth="769.0">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TableColumn fx:id="idCol" prefWidth="75.0" text="Member's ID" />
                  <TableColumn fx:id="nameCol" prefWidth="75.0" text="Full Name" />
                  <TableColumn fx:id="mobileCol" prefWidth="75.0" text="Mobile" />
                  <TableColumn fx:id="emailCol" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="passwordCol" prefWidth="75.0" text="Password" />
               </columns>
               <contextMenu>
                  <ContextMenu>
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#handleMemberEdit" text="Edit" />
                        <MenuItem mnemonicParsing="false" onAction="#handleMemberDelete" text="Delete" />
                        <MenuItem mnemonicParsing="false" onAction="#handleRefresh" text="Refresh" />
                    </items>
                  </ContextMenu>
               </contextMenu>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
